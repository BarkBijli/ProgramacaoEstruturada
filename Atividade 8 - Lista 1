#include <stdio.h>

int main() {
    int valores[6];
    int i;

    // Solicitando ao usu√°rio que insira os valores
    printf("Digite 6 valores inteiros:\n");

    // Lendo os valores e armazenando no vetor
    for (i = 0; i < 6; i++) {
        scanf("%d", &valores[i]);
    }

    // Mostrando os valores na ordem inversa
    printf("Valores na ordem inversa:\n");
    for (i = 5; i >= 0; i--) {
        printf("%d\n", valores[i]);
    }

    return 0;
}
